    {
      "time": "00:19.48",
      "timestamp": 19.48,
      "text": "終わりなき旅の途中　立ち止まりそうな時"
    },
    {
      "time": "00:27.16",
      "timestamp": 27.16,
      "text": "深いため息こぼす　僕らは"
    },
    {
      "time": "00:35.41",
      "timestamp": 35.41,
      "text": "掴みかけてはまた離れていった"
    },
    {
      "time": "00:41.57",
      "timestamp": 41.57,
      "text": "でも今はもう何も恐れることはない"
    },
    {
      "time": "00:49.16",
      "timestamp": 49.16,
      "text": "心を繋ぐ強い絆は"
    },
    {
      "time": "00:56.67",
      "timestamp": 56.67,
      "text": "けして解けはしないさ"
    },
    {
      "time": "01:04.21",
      "timestamp": 64.21,
      "text": "探し続けて辿り着いた"
    },
    {
      "time": "01:12.01",
      "timestamp": 72.01,
      "text": "この場所でPeriodに"
    },
    {
      "time": "01:18.90",
      "timestamp": 78.9,
      "text": ""
    },
    {
      "time": "01:34.99",
      "timestamp": 94.99,
      "text": "愛に満ちたぬくもり　体中駆け巡る"
    },
    {
      "time": "01:42.75",
      "timestamp": 102.75,
      "text": "二度と消えることない　記憶が…"
    },
    {
      "time": "01:49.60",
      "timestamp": 109.6,
      "text": "必ず約束果たして君を連れてくよ"
    },
    {
      "time": "01:57.41",
      "timestamp": 117.41,
      "text": "溢れる(夢を)願い(抱いて)すべて歌に乗せて"
    },
    {
      "time": "02:04.70",
      "timestamp": 124.7,
      "text": "現実から目を逸らさずに"
    },
    {
      "time": "02:12.28",
      "timestamp": 132.28,
      "text": "立ち向かう勇気を"
    },
    {
      "time": "02:19.80",
      "timestamp": 139.8,
      "text": "悲しみ怒り力に変えて"
    },
    {
      "time": "02:27.63",
      "timestamp": 147.63,
      "text": "運命はすぐそばに"
    },
    {
      "time": "02:36.86",
      "timestamp": 156.86,
      "text": "向かう先はほら"
    },
    {
      "time": "02:42.51",
      "timestamp": 162.51,
      "text": "煌めく世界"
    },
    {
      "time": "02:49.82",
      "timestamp": 169.82,
      "text": "僕を震わすこの歓声を"
    },
    {
      "time": "02:57.61",
      "timestamp": 177.61,
      "text": "裏切ることはしないさ"
    },
    {
      "time": "03:05.13",
      "timestamp": 185.13,
      "text": "共に生きてく未来の為に"
    },
    {
      "time": "03:12.99",
      "timestamp": 192.99,
      "text": "この場所でPeriodに"
    },
    {
      "time": "03:20.43",
      "timestamp": 200.43,
      "text": "真実は指先に"
    },
